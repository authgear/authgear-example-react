{
  "name": "authgear-react-tutorial-demoapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel serve --port 4000 --no-cache ./src/index.html",
    "build": "parcel build --no-cache ./src/index.html",
    "typecheck": "tsc",
    "format": "prettier --list-different --write './src/**/*.{js,ts,jsx,tsx,css,scss,html}'",
    "eslint": "eslint './src/**/*.{js,ts,jsx,tsx}'"
  },
  "alias": {
    "process": false,
    "buffer": false
  },
  "devDependencies": {
    "@oursky/eslint-plugin": "8.0.0",
    "@types/node": "16.11.43",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "eslint": "8.11.0",
    "parcel": "2.6.2",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@authgear/web": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
